    "def right_side(array):\n",
    "  right = []\n",
    "  for i in range(len(array)):\n",
    "    if i % 2 == 0:\n",
    "      right.append(array[i])\n",
    "  return right\n",
    "\n",
    "print(right_side([1,2,3,4,5,6,7,8,9]))"
   ]
